{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-underscore-dangle": "off",
        "no-param-reassign": "off",
        "no-use-before-define": "off",
        "no-restricted-syntax": "off",
        "array-callback-return": "off",
        "new-cap": "off",
        "no-mixed-operators": "off",
        "consistent-return": "off",
        "no-unused-vars": "warn"
    }
}